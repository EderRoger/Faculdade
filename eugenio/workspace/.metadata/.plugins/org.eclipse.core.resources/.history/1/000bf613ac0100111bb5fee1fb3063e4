package teste;

import main.Calculador;
import junit.framework.TestCase;

public class TesteAdapter extends TestCase {

	Calculador teste = new Calculador();

	public void testeCalculoDeRaiz() {
		assertEquals("2",2, (int) teste.calcularRaiz(4));
	}

	public void testeCalculoDePotencia() {
		assertEquals("100",100, (int) teste.calcularPotencia(10, 2));
	}
}
