package teste;

import grafo.GeradorDeRotas;
import junit.framework.TestCase;

public class TesteGrafos extends TestCase {

	public void testDirecoesEntreCidadeAEB(){
		GeradorDeRotas geradorDeRotas =  new GeradorDeRotas();
		assertEquals(9, geradorDeRotas.calculaRotasEntreAsCidades("A","B"));
	}
	
}
